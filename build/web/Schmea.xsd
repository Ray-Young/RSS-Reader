<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified">
    
    <xs:element name="root">
        <xs:complexType> 
            <xs:sequence>
                <xs:element name="news" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="publisher_title" type="xs:string"/>
                            <xs:element name="news_title" type="xs:string"/>
                            <xs:element name="news_link" type="xs:string"/>
                            <xs:element name="publish_time" type="xs:string"/>
                        </xs:sequence>
                        <xs:attribute name="newsNo" type="xs:integer"/>                   
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
